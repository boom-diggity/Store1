#include <iostream> 
#include <algorithm>	
#include <vector>
#include <string>
using namespace std;
void printElement(const vector<int> &arr, int length)
//void printElement(int(&arr)[5]) // []안에 element 수가 들어가야한다.
{
	for (int index = 0; index < length; ++index)
	{
		cout << arr[index] << " ";
	}cout << endl;
}

int main()

{
	/*int arr[]{ 1,2,3,4,5 };*/

	vector < int > arr{ 1,2,3,4 };
	printElement(arr, 4);

	return 0;
}
